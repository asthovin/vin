}

// *****problem no. 5

#include<iostream>
using namespace std;
int main(){
    float l,w,h;
    cout<<" Enter the length,width,height of the cuboid ";
    cin>>l>>w>>h;
    cout<<" the volume of the cuboid is "<< l*w*h;
    return 0;
}


