// *****problem no. 8

#include<iostream>
using namespace std;
int main(){
    int a,b;
    cout<<" Enter the two variables  ";
    cin>>a>>b;
    cout<<"THe unswaped nums are :- "<<a << "and"<<b;
    a=a+b;
    b=a-b;
    a=a-b;
    cout<<" the swaped nums are :- "<< a<<" and "<<b;
    return 0;
}
// *****problem no. 9

#include<iostream>
using namespace std;
int main(){
    int a,b;
    cout<<" Enter the two nums   ";
    cin>>a>>b;


    if (a>b)
    
    cout<<" the maximum is  "<< a;
    
    else
        cout<<" the maximum is  "<<b;


    return 0;
}


// problem no. 10
#include<iostream>
using namespace std;
int main ()
{
    int arr[10], n, i, sum = 0, pro = 1;
    cout << "Enter the size of the array : ";
    cin >> n;
    cout << "\nEnter the elements of the array : ";
    for (i = 0; i < n; i++)
    cin >> arr[i];
    for (i = 0; i < n; i++)
    {
        sum += arr[i];
    }
    cout << "\nSum of array elements : " << sum;
    return 0;
}
